<div class="container">
  <div class="col-lg-6 col-md-offset-3">
    <div class="row">
     <h1>Collections#index</h1>
    </div>
    <div class="row">
      <% @collections.each do |collection| %>
      <div class="panel panel-default ">
        <div class="panel-body">
          <%= link_to collection.name, user_collection_path(@user,collection) %>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>


